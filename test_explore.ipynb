    "def top_posts(sim, ri, n, users, posts):\n",
    "    top_n = list(range(-1,-n-1,-1))\n",
    "    doc = sim[ri, :]\n",
    "    doc_sort = np.argsort(doc)\n",
    "    doc_sort = doc_sort[0:-1]\n",
    "    user = users[ri]\n",
    "    sim_users = doc_sort[top_n]\n",
    "    return (user, posts[ri]), [(users[sim], posts[sim]) for sim in sim_users]"
